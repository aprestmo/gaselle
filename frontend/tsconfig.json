{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@atoms/*": ["src/atoms/*"],
      "@data/*": ["src/data/*"],
      "@icons/*": ["src/icons/*"],
      "@js/*": ["src/js/*"],
      "@lib/*": ["src/lib/*"],
      "@molecules/*": ["src/molecules/*"],
      "@organisms/*": ["src/organisms/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "@templates/*": ["src/templates/*"],
    }
  }
}
